{
  "name": "reactjs-webpack4-babel7",
  "version": "1.0.0",
  "description": "Using webpack 4 and babel 7 to bundle Javascript files for usage in React JS",
  "repository": {
    "type": "git",
    "url": "git://github.com/dangduoc08/reactjs.git"
  },
  "author": "Ta Dang Duoc",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf *_build",
    "prebuild": "yarn -s clean",
    "build": "webpack --config=./configs/webpack.prod.js --progress",
    "postbuild": "npx babel --env-name production server --out-dir server_build",
    "start:dev": "NODE_ENV=development nodemon --watch server --watch config --exec babel-node server",
    "start:prod": "NODE_ENV=production node server_build --port=9090"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "express": "^4.17.1",
    "express-static-gzip": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-minify": "^0.5.1",
    "compression-webpack-plugin": "^3.0.0",
    "core-js": "^3.2.1",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "regenerator-runtime": "^0.13.3",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
